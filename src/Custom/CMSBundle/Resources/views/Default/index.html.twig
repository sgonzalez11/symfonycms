{% extends "::base.html.twig" %}

{% block body %}
    {%  for page in pages %}
        <div class="page">
            <h1>{{ page.title }}</h1>
            <p>{{page.content | slice(0,25)}}... <a href="{{ path('custom_cms_homepage') }}">Read More</a></p>
        </div>
    {%  endfor %}
{% endblock %}
    